@model IEnumerable<Information_System_Of_The_Project_Organization.Models.SubcontractorWorksInfo>

@{
    ViewData["Title"] = "Subcontractor Works Info";
}

<h1>Subcontractor Works Info</h1>

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.NameOrganization)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TypeOfOrder)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.NameProject)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Cost)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.NameOrganization)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TypeOfOrder)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.NameProject)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Cost)
            </td>
        </tr>
}
    </tbody>
</table>
